{
  "name": "cors-unlocker",
  "version": "1.0.0",
  "description": "Lightweight npm package for seamless CORS management through the CORS Unlocker browser extension. Perfect for API testing and cross-origin development.",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "sleep 8 && pnpm dev:watch",
    "dev:watch": "vite build --watch --mode development",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cors", 
    "cross-origin", 
    "api-testing", 
    "browser-extension", 
    "http-headers", 
    "fetch", 
    "ajax", 
    "web-development", 
    "typescript",
    "lightweight"
  ],
  "author": "Saiya",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "catalog:",
    "@types/node": "^22.10.1",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "^4.5.3"
  }
}
